{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "server/server.js"
    },
    {
      "src": "/docs/(.*)",
      "dest": "docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ]
}